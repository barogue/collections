{
    "name": "barogue/collection",
    "description": "A class wrapper for PHP native arrays",
    "type": "library",
    "license": "mit",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "php",
        "barogue",
        "arrays",
        "collections",
        "collection"
    ],
    "require": {
        "php": ">=8.1",
        "barogue/arrays": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phan/phan": "*",
        "phpunit/phpunit": "*",
        "phpbench/phpbench": "*",
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "Barogue\\Collections\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Barogue\\Collections\\Tests\\": "tests/"
        }
    }
}
